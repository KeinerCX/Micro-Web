{
  "name": "micro",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel --global-deps='.env'",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "schema:gen": "prisma generate",
    "schema:push": "prisma db push"
  },
  "devDependencies": {
    "prettier": "latest",
    "prisma": "^3.13.0",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "npm@8.5.5",
  "dependencies": {
    "@prisma/client": "^3.13.0"
  }
}
